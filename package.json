{
  "name": "var-translation",
  "displayName": "驼峰翻译助手",
  "description": "纠结怎么取变量? 中文一键翻译转换成常用大小驼峰等格式",
  "publisher": "svenzhao",
  "icon": "images/logo.png",
  "version": "0.13.4",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translate",
    "variable",
    "camel",
    "var",
    "翻译",
    "变量"
  ],
  "activationEvents": [
    "onCommand:extension.varTranslation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.varTranslation",
        "key": "Alt+shift+t",
        "mac": "cmd+shift+t"
      }
    ],
    "commands": [
      {
        "command": "extension.varTranslation",
        "title": "varTranslation",
        "when": "editorTextFocus",
        "category": "var-translation"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.varTranslation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "varTranslation configuration",
      "properties": {
        "varTranslation.translationEngine": {
          "type": "string",
          "enum": [
            "googleCN",
            "google",
            "youdao",
            "baidu"
          ],
          "enumDescriptions": [
            "谷歌中国",
            "谷歌",
            "有道",
            "百度(会抽风)"
          ],
          "default": "'googleCN'",
          "description": "translationEngine"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run release && npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^6.8.0",
    "eslint-config-ali": "^11.1.1",
    "eslint-plugin-import": "^2.22.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "standard-version": "^8.0.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "translation.js": "^0.7.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SvenZhao/var-translation"
  },
  "homepage": "https://github.com/SvenZhao/var-translation/blob/master/README.md"
}
