{
  "name": "var-translation",
  "displayName": "驼峰翻译助手",
  "description": "纠结怎么取变量? 中文一键翻译转换成常用大小驼峰等格式",
  "publisher": "svenzhao",
  "icon": "images/logo.png",
  "version": "0.13.19",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translate",
    "variable",
    "camel",
    "var",
    "翻译",
    "变量"
  ],
  "activationEvents": [
    "onCommand:extension.varTranslation"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.varTranslation",
        "key": "Alt+shift+t",
        "mac": "cmd+shift+t"
      }
    ],
    "commands": [
      {
        "command": "extension.varTranslation",
        "title": "varTranslation",
        "when": "editorTextFocus",
        "category": "var-translation"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.varTranslation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "varTranslation configuration",
      "properties": {
        "varTranslation.translationEngine": {
          "type": "string",
          "enum": [
            "google",
            "microsoft",
            "yandex",
            "baidu"
          ],
          "enumDescriptions": [
            "谷歌中国",
            "微软",
            "yandex(慢)",
            "百度(抽风)"
          ],
          "default": "'google'",
          "description": "translationEngine"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run release && npm run package",
    "compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/google-translate-api": "^2.3.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "standard-version": "^9.1.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "translate-platforms": "0.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SvenZhao/var-translation"
  },
  "homepage": "https://github.com/SvenZhao/var-translation/blob/master/README.md"
}
